<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <!-- Séquence pour les références d'inventaire -->
    <record id="sequence_stockex_inventory" model="ir.sequence">
        <field name="name">Séquence Inventaire Stock</field>
        <field name="code">stockex.stock.inventory</field>
        <field name="prefix">INV/%(year)s/</field>
        <field name="padding">5</field>
        <field name="number_increment">1</field>
        <field name="number_next">1</field>
        <field name="implementation">standard</field>
        <field name="use_date_range">False</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>
